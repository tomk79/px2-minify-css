        "php": ">=5.4.0",
        "tubalmartin/cssmin": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*" ,
        "pickles2/px-fw-2.x": "dev-develop"
    },
    "autoload": {
        "files": [
            "php/css-min.php"
        ]
    },
    "extra": {
        "px2package": {
            "name": "tomk79/px2-minify-css",
            "type": "processor"
        }
    }
