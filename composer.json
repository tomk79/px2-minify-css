{
    "name": "tomk79/px2-minify-css",
    "description": "CSSファイルを圧縮する Pickles 2 プラグイン。",
    "keywords": ["pickles2"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tomoya Koyanagi",
            "email": "tomk79@gmail.com"
        }
    ],
    "scripts":{
        "test": [
            "php ./vendor/phpunit/phpunit/phpunit"
        ]
    } ,
    "repositories": [
    ],
